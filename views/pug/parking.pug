              h5.card-title.title Taxis

          .card.slide-div
            img#img3.img(src='../../images/coaster.jpeg' alt='...') 
            .card-body
              h5.card-title.title Coaster
              
          .card.slide-div
            img#img4.img(src='../../images/pcar.jpeg' alt='...')
            .card-body
              h5.card-title.title Personal car
              
          .card.slide-div
            img#img5.img(src='../../images/boda.jpeg' alt='...')
            .card-body
              h5.card-title.title Boda-boda
              
